<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

<link rel="stylesheet" href="/dashboardstyle.css">


{{#if currentUser.firstTime}}
<div class="modelbox animated bounceInDown">
  <h1 class="whiteout" align="center">Welcome <span style="color: #ecf0f1">{{currentUser.username}}!</span></h1>
  <h2 class="whiteout" align="center">It's your first time here! Please visit the <a style="color: #ecf0f1" href="/firstvisit">Profile Builder</a> to move on!</h2>
  <a style="margin-top: 30px; margin-left: 100px; margin-right: 100px;" role="button" class="btn btn-primary" href="/firstvisit">PROFILE BUILDER</a>
</div>
{{else}}
<h1 class="whiteout" align="center">{{currentUser.firstname}}'s Dashboard!</h1>
{{/if}}


{{#if currentUser.beginner}}
<h1 class="whiteout" align="center">You are in the Beginners web course.</h1>
{{else if currentUser.intermediate}}
<h1 class="whiteout" align="center">You are in the Intermediate web course.</h1>
{{else if currentUser.advanced}}
<h1 class="whiteout" align="center">You are in the Advanced web course.</h1>
{{else}}
<h1 class="whiteout" align="center">You are not currently enrolled in a track.</h1>
{{/if}}








<h1 style="display: none;" align="center" id="typed" class="root1Text"></h1>
